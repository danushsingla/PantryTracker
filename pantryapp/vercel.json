{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      },
      {
        "src": "api/detect.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "source": "/api/(.*)",
        "destination": "/api/detect"
      }
    ]
  }
  