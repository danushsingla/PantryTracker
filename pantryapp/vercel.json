{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      },
      {
        "src": "api/detect.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "2000mb",
          "runtime": "python3.8"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/detect.py"
      }
    ]
  }
  