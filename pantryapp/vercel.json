{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      },
      {
        "src": "api/detect.py",
        "use": "@vercel/python"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://pantry-tracker-phi.vercel.app/api/detect"
      },
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }
  